                                @Html.DropDownListFor(m => m.Record.LanguageSecond, new SelectList(EnumUtils.GetDescriptions<ManagementEnums.Generic.Language>()), new { @class = "form-control" })
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label">Filing Date Value</label>
                            <div class="col-sm-4">
                                @Html.TextBoxFor(m => m.Record.FilingDateValue, new { @class = "form-control required", @type = "date" })
