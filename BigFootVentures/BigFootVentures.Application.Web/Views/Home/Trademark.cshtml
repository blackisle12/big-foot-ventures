<link href="~/Content/Views/Home/Trademark.css" rel="stylesheet" />

@using BigFootVentures.Application.Web.Models
@using BigFootVentures.Application.Web.Models.Utilities
@using BigFootVentures.Application.Web.Models.ViewModels
@using BigFootVentures.Business.Objects.Enumerators
@using BigFootVentures.Business.Objects.Management
@using BigFootVentures.Business.Objects.Utilities

@{
    ViewBag.Title = "Trademark - " + (Model.Record.Name ?? "New Record");
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@model VMModel<Trademark>

@using (Html.BeginForm("Trademark", "Home", FormMethod.Post, new { @class = "form-horizontal" }))
{
    @Html.HiddenFor(m => m.Record.ID)
    @Html.HiddenFor(m => m.Record.Office.ID)
    @Html.HiddenFor(m => m.Record.Brand.ID)
    @Html.HiddenFor(m => m.Record.OriginalOffice.ID)
    @Html.HiddenFor(m => m.Record.TMWebsite.ID)
    @Html.HiddenFor(m => m.Record.OwnerWebsite.ID)
    @Html.HiddenFor(m => m.Record.ComWebsite.ID)
    @Html.HiddenFor(m => m.Record.InvalidityApplicant.ID)

    @Html.Partial("../Shared/_Components/Page/_PageEditViewAlert")

    <div class="panel panel-default panel-parent">
        <div class="panel-heading">
            <span class="panel-title">
                You are @(Model.Record.ID > 0 ? "editing" : "creating new") Trademark <strong>@Model.Record.Name</strong>
            </span>
            <span class="panel-title pull-right">
                <i class="fa fa-edit"></i>
            </span>
        </div>
        <div class="panel-body">
            <!-- START: Trademark Office -->
            <div class="panel panel-default">
                <div class="panel-heading">
                    <h4 class="panel-title">Trademark Office</h4>
                </div>
                <div class="panel-body">
                    <fieldset>
                        <div class="form-group">
                            <label class="col-sm-2 control-label">Trademark Name</label>
                            <div class="col-sm-10">
                                @Html.TextBoxFor(m => m.Record.Name, new { @class = "form-control required" })
                                @Html.ValidationMessageFor(m => m.Record.Name, "", new { @class = "text-danger" })
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label">Office</label>
                            <div class="col-sm-10">
                                <div class="input-group">
                                    @Html.TextBoxFor(m => m.Record.Office.OfficeName, new { @class = "form-control required", @readonly = "readonly" })
                                    <div class="input-group-btn">
                                        <button type="button" name="Button.Office.OfficeName" class="btn btn-default"><i class="fa fa-address-book-o"></i></button>
                                    </div>
                                </div>
                                @Html.ValidationMessageFor(m => m.Record.Office.OfficeName, "", new { @class = "text-danger" })
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label">Office Status</label>
                            <div class="col-sm-10">
                                @Html.DropDownListFor(m => m.Record.OfficeStatus, new SelectList(EnumUtils.GetDescriptions<ManagementEnums.Trademark.OfficeStatus>()), new { @class = "form-control" })
                            </div>
                        </div>
                    </fieldset>
                </div>
            </div>
            <!-- END: Trademark Office -->
            <!-- START: Trademark Information -->
            <div class="panel panel-default">
                <div class="panel-heading">
                    <h4 class="panel-title">Trademark Information</h4>
                </div>
                <div class="panel-body">
                    <fieldset>
                        <div class="form-group">
                            <label class="col-sm-2 control-label">Trademark Number</label>
                            <div class="col-sm-4">
                                @Html.TextBoxFor(m => m.Record.TrademarkNumber, new { @class = "form-control required" })
                                @Html.ValidationMessageFor(m => m.Record.TrademarkNumber, "", new { @class = "text-danger" })
                            </div>

                            <label class="col-sm-2 control-label text-neutral">MisMatch Classes Checkbox</label>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label">International Registration Number</label>
                            <div class="col-sm-4">
                                @Html.TextAreaFor(m => m.Record.InternationalRegistrationNumber, new { @class = "form-control" })
                            </div>

                            <label class="col-sm-2 control-label">Status Grouping</label>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label">Filing Number</label>
                            <div class="col-sm-4">
                                @Html.TextAreaFor(m => m.Record.FilingNumber, new { @class = "form-control required" })
                                @Html.ValidationMessageFor(m => m.Record.FilingNumber, "", new { @class = "text-danger" })
                            </div>

                            <label class="col-sm-2 control-label text-neutral">Office Status Description</label>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label">Registration Number</label>
                            <div class="col-sm-4">
                                @Html.TextAreaFor(m => m.Record.RegistrationNumber, new { @class = "form-control" })
                            </div>

