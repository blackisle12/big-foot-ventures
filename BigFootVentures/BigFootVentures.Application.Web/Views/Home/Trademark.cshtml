                <div class="panel-heading">
                    <h4 class="panel-title">Trademark Detail Office Specific Fields</h4>
                </div>
                <div class="panel-body">
                    <fieldset>
                        <div class="form-group display-flex">
                            <label class="col-sm-2 control-label">WIPO Designated Protection</label>
                            <div class="col-sm-4">
                                @Html.ListBoxFor(m => m.Record.WIPODesignatedProtectionsAvailable, InputUtils.GetMultiSelectList(EnumUtils.GetDescriptions<ManagementEnums.Trademark.WIPO>(Model.Record.WIPODesignatedProtections)), new { @class = "form-control pick-list-box" })
                            </div>
                            <div class="col-sm-1 vertical-align">
                                <button type="button" name="Button.Add.WIPODesignatedProtection" class="btn btn-default pick-list-box-btn"><i class="fa fa-chevron-right"></i></button>
                                <button type="button" name="Button.Remove.WIPODesignatedProtection" class="btn btn-default pick-list-box-btn"><i class="fa fa-chevron-left"></i></button>
                            </div>
                            <div class="col-sm-4">
                                @Html.ListBoxFor(m => m.Record.WIPODesignatedProtectionsSelected, InputUtils.GetMultiSelectList(Model.Record.WIPODesignatedProtections), new { @class = "form-control pick-list-box" })
                            </div>
                        </div>
                        <div class="form-group display-flex">
