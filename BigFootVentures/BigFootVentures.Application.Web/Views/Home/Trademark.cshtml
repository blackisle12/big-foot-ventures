                            <div class="col-sm-4">
                                @Html.ListBoxFor(m => m.Record.WIPOBasicApplicationISOsAvailable, InputUtils.GetMultiSelectList(EnumUtils.GetDescriptions<ManagementEnums.Trademark.WIPO>(Model.Record.WIPOBasicApplicationISOs)), new { @class = "form-control pick-list-box" })
