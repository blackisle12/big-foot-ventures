                                @Html.CheckBoxFor(m => m.Record.Figurative)
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label">Brand</label>
                            <div class="col-sm-4">
                                <div class="input-group">
                                    @Html.TextBoxFor(m => m.Record.Brand.Name, new { @class = "form-control required", @readonly = "readonly" })
                                    <div class="input-group-btn">
                                        <button type="button" name="Button.Brand.Name" class="btn btn-default"><i class="fa fa-address-book-o"></i></button>
                                    </div>
                                </div>
                                @Html.ValidationMessageFor(m => m.Record.Brand.Name, "", new { @class = "text-danger" })
                            </div>

                            <label class="col-sm-2 control-label">Trademark Type</label>
                            <div class="col-sm-4">
                                @Html.DropDownListFor(m => m.Record.TrademarkType, new SelectList(EnumUtils.GetDescriptions<ManagementEnums.Trademark.TrademarkType>()), new { @class = "form-control required" })
                                @Html.ValidationMessageFor(m => m.Record.TrademarkType, "", new { @class = "text-danger" })
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label">Owner</label>
                            <div class="col-sm-4">
                                @Html.TextBoxFor(m => m.Record.OwnerName, new { @class = "form-control", @readonly = "readonly" })
                            </div>

                            <label class="col-sm-2 control-label">Figurative URL</label>
                            <div class="col-sm-4">
                                @Html.TextBoxFor(m => m.Record.FigurativeURL, new { @class = "form-control" })
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label text-neutral">Original Office</label>
                            <div class="col-sm-4">
                                <div class="input-group">
                                    @Html.TextBoxFor(m => m.Record.OriginalOffice.OfficeName, new { @class = "form-control input-neutral", @readonly = "readonly" })
                                    <div class="input-group-btn">
                                        <button type="button" name="Button.OriginalOffice.OfficeName" class="btn btn-default"><i class="fa fa-address-book-o"></i></button>
                                    </div>                                    
                                </div>
                            </div>

                            <label class="col-sm-2 control-label">Language Filing</label>
                            <div class="col-sm-4">
                                @Html.DropDownListFor(m => m.Record.LanguageFiling, new SelectList(EnumUtils.GetDescriptions<ManagementEnums.Generic.Language>()), new { @class = "form-control required" })
