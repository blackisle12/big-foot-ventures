                        <div class="form-group">
                            <label class="col-sm-2 control-label">Revocation Reference External</label>
                            <div class="col-sm-10">
                                @Html.TextBoxFor(m => m.Record.RevocationReferenceExternal, new { @class = "form-control" })
                            </div>
                        </div>
                    </fieldset>
                </div>
            </div>
            <!-- END: Cancellation Store -->
            <!-- START: Invalidity -->
            <div class="panel panel-default">
                <div class="panel-heading">
                    <h4 class="panel-title">Invalidity</h4>
                </div>
                <div class="panel-body">
                    <fieldset>
                        <div class="form-group">
                            <label class="col-sm-2 control-label">Invalidity Number</label>
                            <div class="col-sm-4">
                                @Html.TextBoxFor(m => m.Record.InvalidityNumber, new { @class = "form-control" })
                            </div>

                            <label class="col-sm-2 control-label">Invalidity Date</label>
                            <div class="col-sm-4">
                                @Html.TextBoxFor(m => m.Record.InvalidityDate, new { @class = "form-control", @type = "date" })
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label">Invalidity Applicant</label>
                            <div class="col-sm-4">
                                <div class="input-group">
                                    @Html.TextBoxFor(m => m.Record.InvalidityApplicant.DisplayName, new { @class = "form-control", @readonly = "readonly" })
                                    <div class="input-group-btn">
                                        <button type="button" name="Button.InvalidityApplicant.DisplayName" class="btn btn-default"><i class="fa fa-address-book-o"></i></button>
                                    </div>
                                </div>
                            </div>

                            <label class="col-sm-2 control-label">Invalidity Action Outcome</label>
                            <div class="col-sm-4">
                                @Html.DropDownListFor(m => m.Record.InvalidityActionOutcome, new SelectList(EnumUtils.GetDescriptions<ManagementEnums.Trademark.InvalidityActionOutcome>()), new { @class = "form-control" })
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label">Invalidity Invoked Ground</label>
                            <div class="col-sm-10">
                                @Html.TextAreaFor(m => m.Record.InvalidityInvokedGround, new { @class = "form-control" })
                            </div>
                        </div>
                    </fieldset>
                </div>
            </div>
            <!-- END: Invalidity -->
            <!-- START: Legal Section -->
            <div class="panel panel-default">
                <div class="panel-heading">
                    <h4 class="panel-title">Legal Section</h4>
                </div>
                <div class="panel-body">
                    <fieldset>
                        <div class="form-group">
                            <label class="col-sm-2 control-label">Letter Reference</label>
                            <div class="col-sm-4">
                                @Html.TextBoxFor(m => m.Record.LetterReference, new { @class = "form-control" })
                            </div>

                            <label class="col-sm-2 control-label">Letter Sent On</label>
                            <div class="col-sm-4">
                                @Html.TextBoxFor(m => m.Record.LetterSentOn, new { @class = "form-control" })
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label">Letter Origin</label>
                            <div class="col-sm-4">
                                @Html.DropDownListFor(m => m.Record.LetterOrigin, new SelectList(EnumUtils.GetDescriptions<ManagementEnums.Trademark.LetterOrigin>()), new { @class = "form-control" })
                            </div>

                            <label class="col-sm-2 control-label">Owner Response Deadline</label>
                            <div class="col-sm-4">
                                @Html.TextBoxFor(m => m.Record.OwnerResponseDeadline, new { @class = "form-control" })
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label">Letter Sending Method</label>
                            <div class="col-sm-4">
                                @Html.DropDownListFor(m => m.Record.LetterSendingMethod, new SelectList(EnumUtils.GetDescriptions<ManagementEnums.Trademark.LetterSendingMethod>()), new { @class = "form-control" })
                            </div>

                            <label class="col-sm-2 control-label">Letter Outcome</label>
                            <div class="col-sm-4">
                                @Html.DropDownListFor(m => m.Record.LetterOutcome, new SelectList(EnumUtils.GetDescriptions<ManagementEnums.Trademark.LetterOutcome>()), new { @class = "form-control" })
                            </div>
                        </div>
                    </fieldset>
                </div>
            </div>
            <!-- END: Legal Section -->
        </div>
        <div class="panel-footer">
            @Html.Partial("../Shared/_Components/Page/_PageEditViewFooter")
        </div>
    </div>

    @Html.Partial("../Shared/_Components/Modals/_AssociateParentChild", new VMAssociation { ParentName = Model.Name, ChildName = nameof(Office), ChildIDContainer = "Office.ID", ChildNameContainer = "Office.OfficeName" })
    @Html.Partial("../Shared/_Components/Modals/_AssociateParentChild", new VMAssociation { ParentName = Model.Name, ChildName = nameof(Brand), ChildIDContainer = "Brand.ID", ChildNameContainer = "Brand.Name" })
    @Html.Partial("../Shared/_Components/Modals/_AssociateParentChild", new VMAssociation { ParentName = Model.Name, ChildName = nameof(DomainN), ChildIDContainer = "TMWebsite.ID", ChildNameContainer = "TMWebsite.Name" })
    @Html.Partial("../Shared/_Components/Modals/_AssociateParentChild", new VMAssociation { ParentName = Model.Name, ChildName = nameof(DomainN), ChildIDContainer = "OwnerWebsite.ID", ChildNameContainer = "OwnerWebsite.Name" })
    @Html.Partial("../Shared/_Components/Modals/_AssociateParentChild", new VMAssociation { ParentName = Model.Name, ChildName = nameof(DomainN), ChildIDContainer = "ComWebsite.ID", ChildNameContainer = "ComWebsite.Name" })
    @Html.Partial("../Shared/_Components/Modals/_AssociateParentChild", new VMAssociation { ParentName = Model.Name, ChildName = nameof(Company), ChildIDContainer = "InvalidityApplicant.ID", ChildNameContainer = "InvalidityApplicant.DisplayName" })

    <script src="~/Scripts/Views/Shared/_Validator.js"></script>
    <script src="~/Scripts/Views/Shared/_PanelHelper.js"></script>
    <script src="~/Scripts/Views/Home/Trademark.js"></script>
}

