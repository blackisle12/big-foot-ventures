                            </div>

                            <label class="col-sm-2 control-label text-neutral">Geography</label>
                            <div class="col-sm-4">
                                @Html.DropDownListFor(m => m.Record.Geography, new SelectList(EnumUtils.GetDescriptions<ManagementEnums.Trademark.Geography>()), new { @class = "form-control input-neutral" })
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label">Publication Date</label>
                            <div class="col-sm-4">
                                @Html.TextBoxFor(m => m.Record.PublicationDate, new { @class = "form-control", @type = "date" })
                            </div>

                            <label class="col-sm-2 control-label">Involved in revocation</label>
                            <div class="col-sm-4">
                                @Html.CheckBoxFor(m => m.Record.InvolvedInRevocation)
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label text-neutral">Registration Date</label>
                            <div class="col-sm-4">
                                @Html.TextBoxFor(m => m.Record.RegistrationDate, new { @class = "form-control input-neutral", @type = "date" })
                            </div>

                            <label class="col-sm-2 control-label">Bigfoot Group Owned</label>
                            <div class="col-sm-4">
                                @Html.DropDownListFor(m => m.Record.BigfootGroupOwned, new SelectList(EnumUtils.GetDescriptions<ManagementEnums.Trademark.BigfootGroupOwned>()), new { @class = "form-control required" })
                                @Html.ValidationMessageFor(m => m.Record.BigfootGroupOwned, "", new { @class = "text-danger" })
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label">Expiry Date</label>
                            <div class="col-sm-4">
                                @Html.TextBoxFor(m => m.Record.ExpiryDate, new { @class = "form-control", @type = "date" })
                            </div>
                        </div>
                        @*<div class="form-group">
                            <label class="col-sm-2 control-label">6 Months Anniversary</label>
                            <div class="col-sm-4">
                                @Html.TextBoxFor(m => m.Record.SixMonthsAnniversary, new { @class = "form-control" })
                            </div>
                        </div>*@
                        <div class="form-group">
                            <label class="col-sm-2 control-label">Priority Date</label>
                            <div class="col-sm-4">
                                @Html.TextBoxFor(m => m.Record.PriorityDate, new { @class = "form-control", @type = "date" })
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label">Priority Country & Priority TM Number</label>
                            <div class="col-sm-4">
                                @Html.TextAreaFor(m => m.Record.PriorityCountryAndPriorityTMNumber, new { @class = "form-control" })
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label">Seniority Used</label>
                            <div class="col-sm-10">
                                @Html.CheckBoxFor(m => m.Record.SeniorityUsed)
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label">Seniority Details</label>
                            <div class="col-sm-10">
                                @Html.TextAreaFor(m => m.Record.SeniorityDetails, new { @class = "form-control" })
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label">Deletion Request</label>
                            <div class="col-sm-10">
                                @Html.CheckBoxFor(m => m.Record.DeletionRequest)
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label">Deletion Request Reason</label>
                            <div class="col-sm-10">
                                @Html.TextAreaFor(m => m.Record.DeletionRequestReason, new { @class = "form-control" })
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label">Revocation Target</label>
                            <div class="col-sm-10">
                                @Html.CheckBoxFor(m => m.Record.RevocationTarget)
                            </div>
                        </div>
                    </fieldset>
                </div>
            </div>
            <!-- END: Trademark Information -->
            <!-- START: Similarity Research -->
            <div class="panel panel-default">
                <div class="panel-heading">
                    <h4 class="panel-title">Similarity Research</h4>
                </div>
                <div class="panel-body">
                    <fieldset>
                        <div class="form-group">
                            <label class="col-sm-2 control-label">Open Similarity Research Task</label>
                            <div class="col-sm-10">
                                @Html.DropDownListFor(m => m.Record.OpenSimilarityResearchTask, new SelectList(EnumUtils.GetDescriptions<ManagementEnums.Trademark.OpenSimilarityResearchTask>()), new { @class = "form-control" })
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label">Initial Submitter</label>
                            <div class="col-sm-10">
                                @Html.TextBoxFor(m => m.Record.InitialSubmitter, new { @class = "form-control" })
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label">Last Similarity Research Completed By</label>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label">Last Similarity Research Completed On</label>
                            <div class="col-sm-10">
                                @Html.TextBoxFor(m => m.Record.LastSimilarityResearchCompletedOn, new { @class = "form-control" })
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label">Opposition Research</label>
                            <div class="col-sm-10">
                                @Html.DropDownListFor(m => m.Record.OppositionResearch, new SelectList(EnumUtils.GetDescriptions<ManagementEnums.Trademark.OppositionResearch>()), new { @class = "form-control" })
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label">Similar Trademark Spellings</label>
                            <div class="col-sm-10">
                                @Html.TextAreaFor(m => m.Record.SimilarTrademarkSpellings, new { @class = "form-control" })
                            </div>
                        </div>
                    </fieldset>
                </div>
            </div>
            <!-- END: Similarity Research -->
            <!-- START: Trademark Detail Office Specific Fields -->
