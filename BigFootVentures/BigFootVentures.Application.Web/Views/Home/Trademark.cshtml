                            </div>
                            <div class="col-sm-4">
                                @Html.ListBoxFor(m => m.Record.ARIPODesignatedStatesSelected, InputUtils.GetMultiSelectList(Model.Record.ARIPODesignatedStates), new { @class = "form-control pick-list-box" })
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label">Trademark Name English Translation</label>
                            <div class="col-sm-10">
                                @Html.TextBoxFor(m => m.Record.TrademarkNameEnglishTranslation, new { @class = "form-control" })
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-2 control-label">WIPO Basic Application - Filing Date</label>
                            <div class="col-sm-10">
                                @Html.TextBoxFor(m => m.Record.WIPOBasicApplicationFilingDate, new { @class = "form-control", @type = "date" })
                            </div>
                        </div>
                        <div class="form-group">
