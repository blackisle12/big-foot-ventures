                            <div class="col-sm-10">
                                @Html.TextBoxFor(m => m.Record.WIPOBasicApplicationNumber, new { @class = "form-control" })
                            </div>
                        </div>
                    </fieldset>
                </div>
            </div>
            <!-- END: Trademark Detail Office Specific Fields -->
            <!-- START: Country Specific - Latvia -->
            <div class="panel panel-default">
                <div class="panel-heading">
